; Things that are actually global to every combination

; :help highlight-groups
MessageWindow        -> Pmenu
PopupNotification    -> Todo
; vim uses SpecialKey for listchar
#if has('nvim')
  SpecialKey         -> Special
#else
  SpecialKey         -> NonText
#endif
StatusLineTerm       -> StatusLine
StatusLineTermNC     -> StatusLineNC
Terminal             -> Normal

; :help lCursor
lCursor              -> Cursor
